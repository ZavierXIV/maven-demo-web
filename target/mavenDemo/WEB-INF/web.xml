<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HelloServlet</servlet-name>
  	<display-name>HelloServlet</display-name>
  	<description></description>
  	<servlet-class>project.HelloServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TeddySevlet</servlet-name>
  	<display-name>TeddySevlet</display-name>
  	<description></description>
  	<servlet-class>project.TeddySevlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloServlet</servlet-name>
  	<url-pattern>/easayhi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TeddySevlet</servlet-name>
  	<url-pattern>/teddysayhi</url-pattern>
  </servlet-mapping>
</web-app>
